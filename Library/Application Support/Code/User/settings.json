{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "none",
  "terminal.integrated.scrollback": 5000,
  "explorer.autoReveal": false,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "node_modules": true,
    "coverage": true,
    "tmp": true,
    ".bundle": true,
    "yarn.lock": true,
    "log": true,
    "spec/examples.txt": true,
    "stripe-mock-server.log": true,
    "public": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.associations": {
    "*.html.erb": "erb"
  },
  "[erb]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "C-r"
      ],
      "commands": [
        "redo"
      ]
    }
  ],
  "ruby.lint": {
    "reek": true,
    "rubocop": true,
    "ruby": true,
    "fasterer": true,
    "debride": true,
    "ruby-lint": true
  },
  "ruby.format": "rubocop",
  "ruby.useLanguageServer": true,
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [
    100
  ],
  "window.title": "${activeEditorMedium}${separator}${folderName}",
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "git.enableCommitSigning": true,
  "material-icon-theme.showUpdateMessage": false,
  "yaml.format.enable": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "html.erb": "html",
    "erb": "html"
  },
  "git.enableSmartCommit": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "window.zoomLevel": 0,
  "git.confirmSync": false,
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "debug.onTaskErrors": "debugAnyway",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "appService.showSavePrompt": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontSize": 15,
  "editor.suggestSelection": "first",
  "workbench.editor.untitled.hint": "hidden",
  "editor.tabSize": 2,
  "workbench.colorTheme": "Nord",
  "explorer.confirmDelete": false,
  "workbench.settings.openDefaultKeybindings": true,
  "github-enterprise.uri": "github.service.anz",
  "terminal.integrated.allowChords": false,
  "workbench.preferredDarkColorTheme": "Nord",
  "window.autoDetectColorScheme": true,
  "workbench.fontAliasing": "auto",
  "gitlens.hovers.enabled": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.chktex.enabled": true,
  //"go.goroot": "/Users/martin/.asdf/installs/golang/1.17.5/go",,
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "go.formatTool": "gofmt",
  "go.lintTool": "golangci-lint",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "sysl.tool.binaryPath": "sysl",
  "redhat.telemetry.enabled": false,
  "editor.bracketPairColorization.enabled": true
}
